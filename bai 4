#include <stdio.h>
int main()
{
	int n;
	int s = 0;
	int count = 0;
	printf("nhap n:");
	scanf_s("%d", &n);
	printf("uoc so cua n la : ");
	int max = 0;
	int min = 0;
	for (int i = 1; i <= n; i++)
	{
		if (n % i == 0)
		{
			printf("%d ", i);
			count++;
			if (i > max && i < n)
			{
				max = i;
			}
			if (min == 0&&i>1)
			{
				min = n;
			}
			if (i < min && i>1)
			{
				min = i;
			}
		}
	}
	printf("\n tong cac uoc so cua n la %d ", count);
	printf("\nuoc so lon nhat cua n la %d ", max);
	printf("\nuoc so nho nhat cua n la %d ", min);


}
