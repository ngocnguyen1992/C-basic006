#include <stdio.h>
int nguyento(int h)
{
	if (h < 2){
		return 0;
	}
	for (int i = 2; i < h; i++){
		if (h % i == 0)
			return 0;
	}
	return 1;
}
int main()
{
    //int n;
    //int count=0;
    int arr[1000];
    int h;
    int m1[1000];
    int m;
    int temp;
  printf("nhap chieu dai cho mang h :");
  scanf("%d",&h);
    printf("nhap gia tri cho mang : ");
    for(int i = 0; i<h;i++){   
        scanf("%d",&arr[i]);
    }
    for(int i = 0;i<h;i++)
    {
        if(nguyento(arr[i]))
        {
            printf("%d ",arr [i]);
        }
    }
}
